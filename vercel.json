 {
    "version":2,
    "builds":[
        {
            "src":"spotify_music/wsgi.py",
            "use":"@vercel/python",
            "confi":{"maxLambdaSize":"15mb","runtime":"python3.12"}
        },
        {
            "src":"build_files.sh",
            "use":"@vercel/static-build",
            "config":{
                "disDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*",
            "dest":"spotify_music/wsgi.py"
        }
    ]

 }